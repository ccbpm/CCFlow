<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="oracle.manageddataaccess.client" type="OracleInternal.Common.ODPMSectionHandler, Oracle.ManagedDataAccess, Version=4.122.19.1, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
    </configSections>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="WSSoap" closeTimeout="00:01:00" openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00" allowCookies="false" bypassProxyOnLocal="false" maxBufferPoolSize="524288" maxReceivedMessageSize="65536" useDefaultWebProxy="true"/>
                <binding name="PortalInterfaceSoap"/>
            </basicHttpBinding>
            <customBinding>
                <binding name="WSSoap12">
                    <textMessageEncoding messageVersion="Soap12"/>
                    <httpTransport realm=""/>
                </binding>
                <binding name="PortalInterfaceSoap12">
                    <textMessageEncoding messageVersion="Soap12"/>
                    <httpTransport/>
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://online.ccflow.org/App/TemplateInterface/WS.asmx" binding="basicHttpBinding" bindingConfiguration="WSSoap" contract="CloundWS.WSSoap" name="WSSoap"/>
            <endpoint address="http://online.ccflow.org/App/TemplateInterface/WS.asmx" binding="customBinding" bindingConfiguration="WSSoap12" contract="CloundWS.WSSoap" name="WSSoap12"/>
            <endpoint address="http://localhost:2207/DataUser/PortalInterface.asmx" binding="basicHttpBinding" bindingConfiguration="PortalInterfaceSoap" contract="CCInterface.PortalInterfaceSoap" name="PortalInterfaceSoap"/>
            <endpoint address="http://localhost:2207/DataUser/PortalInterface.asmx" binding="customBinding" bindingConfiguration="PortalInterfaceSoap12" contract="CCInterface.PortalInterfaceSoap" name="PortalInterfaceSoap12"/>
        </client>
    </system.serviceModel>

 
  
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup>
<system.data>
<DbProviderFactories>
<remove invariant="Oracle.ManagedDataAccess.Client"/>
<add name="ODP.NET, Managed Driver" invariant="Oracle.ManagedDataAccess.Client" description="Oracle Data Provider for .NET, Managed Driver" type="Oracle.ManagedDataAccess.Client.OracleClientFactory, Oracle.ManagedDataAccess, Version=4.122.19.1, Culture=neutral, PublicKeyToken=89b483f429c47342"/>
</DbProviderFactories>
</system.data>
<runtime>
<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
<dependentAssembly>
<publisherPolicy apply="no"/>
<assemblyIdentity name="Oracle.ManagedDataAccess" publicKeyToken="89b483f429c47342" culture="neutral"/>
<bindingRedirect oldVersion="4.121.0.0 - 4.65535.65535.65535" newVersion="4.122.19.1"/>
</dependentAssembly>
</assemblyBinding>
</runtime>
<oracle.manageddataaccess.client>
<version number="*">
