<?xml version="1.0" encoding="utf-8" ?>
<DS>
  <MapMenu No="ExpImp" CH="导入/导出" JS="javascript:ExpImp(@MyPK,@FK_Flow);" Img="./Img/MapExt/ExpImp.png"
         Note="导入或者导入表单模板。" />

  <MapMenu No="NewF" CH="新建字段"   JS="javascript:AddF(@MyPK);"  Img="./Img/Btn/New.gif"
           Note="向表单中增加新字段。" />

  <MapMenu No="HidAttr" CH="隐藏字段" JS="javascript:HidAttr(@MyPK);" Img="./Img/MapExt/HiddenField.png"
            Note="多用于计算、方向条件设置。" />

  <MapMenu No="FGroup" CH="新建字段分组" JS="javascript:GroupFieldNew(@MyPK);" Img="./Img/MapExt/GroupBar.png"
           Note="把同类的字段分割开,让显示更有条理." />

  <!--<MapMenu No="CopyF" CH="复制字段"  JS="javascript:CopyFieldFromNode(@MyPK);"  Img="./Img/Btn/Copy.gif"
           Note="从其它表单中复制字段" />-->

  <MapMenu No="DTable" CH="新建从表" JS="javascript:MapDtl(@MyPK);" Img="./Img/MapExt/Dtl.png"
           Note="也称从表,如:报销费用清单."  />

  <MapMenu No="Ath" CH="新建附件组件"     JS="javascript:Ath(@MyPK);" Img="./Img/MapExt/Attachment.png"
           Note="单个附件与多个附件类型的数据上传、浏览。"/>

  <MapMenu No="Frm" CH="设置框架"    JS="javascript:MapFrame(@MyPK);" Img="./Img/Frame.gif"
           Note="框架嵌入自定义URL"/>

  <MapMenu No="FrmEvent" CH="表单事件"    JS="javascript:FrmEvent(@MyPK);" Img="./Img/MapExt/Event.png"
             Note="可以实现特殊的应用需求."/>

  <MapMenu11 No="MapExt" CH="扩展设置"    JS="javascript:MapExt(@MyPK);" Img="./Img/MapExt/DTS.gif"
          Note="表单的业务逻辑，实现特殊的应用需求."/>

  <MapMenu111 No="Sln" CH="权限控制方案"    JS="javascript:Sln(@MyPK);" Img="./Img/MapExt/DTS.gif"
          Note="可以实现不用的节点对不同字段的权限访问."/>

  <MapMenu No="MapExt" CH="表单属性"    JS="javascript:MapDataEdit(@MyPK);" Img="./Img/MapExt/DTS.gif"
            Note="更改表单的基础信息，映射的物理表，展示格式..."/>

  <MapMenu No="BatchEdit" CH="批量修改"    JS="javascript:BatchEdit(@MyPK);" Img="./Img/MapExt/DTS.gif"
            Note="批量修改字段属性..."/>

  <!--<MapMenu111 No="Ext" CH="扩展设置"    JS="javascript:MapExt(@MyPK);" Img="./Img/Btn/DTS.gif" />-->

  <FExt No="Auto" CH="自动填充" DFor="TB,DDL,Checkbox" />
  <FExt No="Auto" CH="自动填充" DFor="TB,DDL,Checkbox" />
  <FExt No="Auto" CH="自动填充" DFor="TB,DDL,Checkbox" />
  <FExt No="Auto" CH="自动填充" DFor="TB,DDL,Checkbox" />
  <FExt No="Auto" CH="自动填充" DFor="TB,DDL,Checkbox" />

  <InputCheck No="01" CH="文本框只能输入数字代码(小数点也不能输入)"    JS="this.value=this.value.replace(/\D/g,'')" />
  <InputCheck No="02" CH="只能输入数字,能输小数点"    JS="this.value=this.value.replace(/\D/g,'')" />

  <FieldGroup No="IsPass" CH="从表审核分组" CHDesc="批量增加三个审核分组字段，是否通过,审核人,审核日期." />
  <FieldGroup No="Eval"   CH="质量评价" CHDesc="批量增加4个审核分组字段，评价人员ID,评价人员名称,评价分值，评价内容." />

  <!-- 表单事件 -->
  <FrmEvent No="FrmLoadBefore" CH="装载前" CHDesc="" />
  <FrmEvent No="FrmLoadAfter" CH="装载后"  CHDesc="" />
  <FrmEvent No="SaveBefore" CH="保存前"    CHDesc="" />
  <FrmEvent No="SaveAfter" CH="保存后"     CHDesc="" />
  <FrmEvent No="DtlSaveEnd" CH="从表保存后"     CHDesc="" />
</DS>

