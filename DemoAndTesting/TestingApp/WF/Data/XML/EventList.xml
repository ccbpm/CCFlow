<?xml version="1.0" standalone="yes" ?>
<NewDataSet>
  
  <!-- 事件类型 -->
  <Source No="Frm"  CH="表单事件"  EN=""/>
  <Source No="Node" CH="节点事件"  EN=""/>
  <Source No="Flow" CH="流程事件"  EN=""/>
  
  <!-- 表单事件 -->
  <Item No="FrmLoadBefore"  CH="节点表单载入前" EN="When the node before saving" EventType="Frm" 
        EventDesc="在载入前，您可以为表单预先执行一些数据初始化。" />
  <Item No="FrmLoadAfter"   CH="节点表单载入后" EN="When the node saved" EventType="Frm" 
        EventDesc="在载入后，执行安全处理。" />
    
  <Item No="SaveBefore"  CH="当节点保存前" EN="When the node before saving"  EventType="Frm"
         EventDesc="可以执行一些数据完整性检查。" />
  <Item No="SaveAfter" CH="当节点保存后" EN="When the node saved" EventType="Frm" 
        EventDesc="可以做相应的业务逻辑处理。"/>

  <!-- 节点事件类型 -->
  <Item No="SendWhen" CH="当节点发送时" EN="When a node is sent" EventType="Node" />
  <Item No="SendSuccess" CH="节点发送成功时" EN="Node has a successful" EventType="Node"  />
  <Item No="SendError" CH="节点发送失败时" EN="Node fails to send"  EventType="Node" />
  <Item No="ReturnBefore" CH="当节点退回前" EN="When the node before returning" EventType="Node" />
  <Item No="ReturnAfter" CH="当节点退回后" EN="When the node returned after" EventType="Node" />
  <Item No="UndoneBefore" CH="当节点撤销发送前" EN="When a node before sending cancellation" EventType="Node" />
  <Item No="UndoneAfter" CH="当节点撤销发送后" EN="When the node after sending cancellation" EventType="Node" />

  <!-- 流程事件类型 -->
	<Item No="FlowOverBefore" CH="流程结束前" EN="When the node after sending cancellation" EventType="Flow" />
	<Item No="FlowOverAfter" CH="流程结束后" EN="When the node after sending cancellation"  EventType="Flow" />
	<Item No="BeforeFlowDel" CH="流程删除前" EN="When the node after sending cancellation" EventType="Flow"  />
	<Item No="AfterFlowDel" CH="流程删除后" EN="When the node after sending cancellation" EventType="Flow"  />

    <!-- 从表事件 -->
    <ItemDtl No="DtlSaveBefore" CH="从表保存前" EN="" EventType="Dtl" />
    <ItemDtl No="DtlSaveEnd" CH="从表保存后" EN=""  EventType="Dtl" />
    <ItemDtl No="DtlItemSaveBefore" CH="单条数据保存前" EN="" EventType="Dtl"  />
    <ItemDtl No="DtlItemSaveAfter" CH="单条数据保存后" EN="" EventType="Dtl"  />
    <ItemDtl No="DtlItemDelBefore" CH="单条删除前" EN="" EventType="Dtl"  />
    <ItemDtl No="DtlItemDelAfter" CH="单条删除后" EN="" EventType="Dtl"  />

</NewDataSet>