<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <!-- 描述webconfig中的配置信息 
  No=是webconfig中配置的Key。
  Name=是它的名称， 
  Note=是描述 DBType是数据类型。 
  IsEnable=是否可以修改它。
  修改web.config中的连接. 功能连接为:\Comm\Sys\EditWebconfig.aspx。  
  其他：要想能够修改web。config，您需要在文件里面设置。
    <identity impersonate="true"  userName="administrator" password="" /> 
    -->
  
  <Item No="IsDebug" Name="是否测试状态" Note="是否测试状态" DBType="Boolen"  IsShow="1" />
  <Item No="IsAuth" Name="是否需要安全验证" Note="在嵌入方式工作时,是否要检查访问者的安全性" DBType="Boolen"  IsShow="1" />

    <Item No="IsEnablePRI" Name="是否为流程增加一个优先级" Note="有的客户不需要流程的优先级，就设置为0" DBType="Boolen"  IsShow="1" />
    <Item No="IsEnableCheckUseSta" Name="对禁用用户立即生效" Note="如果应用系统在需要禁用用户后立即生效的话就设置为1，设置为1后会经常检测，增加系统一些开销。"
          DBType="Boolen"  IsShow="1" />

    <Item No="SysLanguage" Name="系统语言" Note="系统语言" DBType="String"   IsEnable="0" />
    <Item No="SysNo" Name="系统编号" Note="系统编号" DBType="String"  IsEnable="0" />
    <Item No="SysName" Name="系统名称"  Note="系统名称" DBType="String"  IsEnable=""  />
    <Item No="CopyRight" Name="版权所有"  Note="显示在系统的底部" DBType="String"  IsEnable="1"  />
    <Item No="TouchWay" Name="联系信息"  Note="显示在系统的底部" DBType="String"  IsEnable="1"  />
  
    <Item No="UIRowStyleGlo" Name="表格风格"  Note="作用于后台组件" DBType="String"  IsEnable="1"  />
    <Item No="UIIsEnableDouclickGlo" Name="启动双击"  Note="启动双击" DBType="String"  IsEnable="1"  />
    <Item No="PageSize" Name="页面大小"  Note="页面大小" DBType="String" />
    <Item No="UITableOrderWay" Name="页面排序方式"  Note="DESC 降序,不输入是升序" DBType="String" />

  <Item No="InitConnNum" Name="系统连接数"  Note="初始化系统的连接数据" DBType="String"  IsEnable="1"  />
  <Item No="AppCenterDSN" Name="应用程序连接"  Note="连接到的数据库密码与用户" DBType="String" />
  <Item No="AppCenterDBType" Name="数据库类型"  Note="支持类型:MSSQL,Oracle, DB2,Access,MySQL" DBType="String"  IsEnable="1" IsShow="0" />
  <Item No="DBAccessOfMSSQL2000" Name=" 辅应用数据MSSQL2000连接"  Note="本系统的名称" DBType="String" IsEnable="0" />

  <Item No="DBAccessOfOLE" Name="辅应用数据Ole连接"  Note="辅应用数据Ole连接  " DBType="String" />
  <Item No="DBAccessOfODBC" Name="辅应用数据 ODBC 连接"  Note="同上" DBType="String" />
  <Item No="DBAccessOfOracle" Name="辅应用数据oracle连接"  Note="同上" DBType="String" />

    <Item No="DeveloperName" Name="开发商"  Note="开发商" DBType="String"   IsEnable="0"  IsShow="0" />
  <Item No="DeveloperShortName" Name=" 开发商"  Note="开发商" DBType="String"  IsEnable="0"   IsShow="0" />
  <Item No="DeveloperTel" Name="电话"  Note="电话" DBType="String"  IsEnable="0"  IsShow="0" />
  <Item No="DeveloperAddr" Name="开发者地址"  Note="" DBType="String"  IsEnable="0"   IsShow="0" />
  <Item No="DeveloperMail" Name="开发者邮件"  Note="" DBType="String"  IsEnable="0"   IsShow="0" />
  
  <Item No="ServiceMail" Name="Tel"  Note="系统用户有问题，或者出现异常就会显示此信息。" DBType="String" />
  <Item No="ServiceTel" Name="E-mail"  Note="同上" DBType="String" />
  
  <Item No="CustomerNo" Name="客户编号"  Note="本系统的名称" DBType="String" IsShow="0" />
  <Item No="CustomerName" Name="客户名称"  Note="本系统的名称" DBType="String" IsShow="0" />
  <Item No="CustomerShortName" Name=" 简称"  Note="本系统的名称" DBType="String" IsShow="0" />
  <Item No="CustomerTel" Name=" 电话"  Note="开发者电话" DBType="String"  IsShow="0" />
  <Item No="CustomerAddr" Name="地址"  Note="开发者" DBType="String"  IsShow="0" />
  
  <Item No="SysIP" Name="系统IP"  Note="" DBType="String" IsEnable="1" />
  <Item No="TopNum" Name="显示条数"  Note="显示条数" DBType="String" />

  <Item No="CIP" Name="群集IP"  Note="服务器运算分布在多台机器上,群集IP." DBType="String" />
  <Item No="FtpIP" Name="文件服务器"  Note="存储系统附件的服务器" DBType="String" />
  <Item No="FtpUser" Name="用户名"  Note="" DBType="String" />
  <Item No="FtpPass" Name="密码"  Note="" DBType="String" />

    <Item No="FileFtpIP" Name="外来资源服务器"  Note="也叫商业资源服务器" DBType="String" />
    <Item No="FileFtpUser" Name="用户名"  Note="" DBType="String" />
    <Item No="FileFtpPass" Name="密码"  Note="" DBType="String" />
  
  <Item No="ServiceTel" Name="客服服务电话" Note="联系方式页面信 客服服务电话" DBType="String" />
  <Item No="Tel" Name="电话" Note="联系方式页面信息 电话" DBType="String" />
  <Item No="Fax" Name="传真" Note="联系方式页面信息 传真" DBType="String" />
  <Item No="WebSite" Name="网址" Note="联系方式页面信息 网址" DBType="String" />
  <Item No="Email" Name="电子邮箱" Note="联系方式页面信息 电子邮箱" DBType="String" />
  <Item No="Address" Name="地址" Note="联系方式页面信息 地址" DBType="String" />
  <Item No="Contacter" Name="联系人" Note="联系方式页面信息 联系人" DBType="String" />


  <Item No="AMFrom" Name="早晨上班时间" Note="以下数据用于计算流程时效考核" DBType="String" IsShow="0"  IsEnable="1" />
  <Item No="AMTo" Name="下班时间"  DBType="String" IsShow="0"  IsEnable="1" />

    <Item No="PMFrom" Name="下午上班时间"  DBType="String" IsShow="0"  IsEnable="1" />
  <Item No="PMTo" Name="下班时间"  DBType="String" IsShow="0"  IsEnable="1" />

  <Item No="WeekResetType" Name="休息"  DBType="Enum" IsShow="0"  IsEnable="1"  Vals="@0=双休@1=单休@2=不休" />
  <Item No="Holiday" Name="节假日"  DBType="String" IsShow="0"  IsEnable="1" Note="格式：05-01,05-02,05-03表示5月前三天是节假日" />

  <Item No="IsShowFlowNum" Name="是否显示待办工作数量？" Note="独立运行模式下提示待办工作数量"
        DBType="Boolen"  IsShow="1" />
  <Item No="IsEnableStyle" Name="是否启用系统风格？" Note="是否允许用户设置系统风格" DBType="Boolen"  IsShow="1" />
  <Item No="BPMHost" Name="流程设计服务器地址" Note="" DBType="String"  IsShow="1" />
</NewDataSet>