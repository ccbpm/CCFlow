                        <Border Margin="0,0,0,0" x:Name="FocusVisualElement" IsHitTestVisible="False" Opacity="0" BorderBrush="{StaticResource YFocusColor}" BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="3"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
